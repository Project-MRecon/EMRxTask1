{
    "json_path": "/homes/syli/dataset/MultiCoil/test/test.json",
    "resume": null,
    "epochs": 100,
    "save_dir": "./saved",
    "network": {"arch": "VarUNet",
                "args": {"spatial_dims": 2,
                        "in_channels": 1,
                        "out_channels": 1,
    "features": [32, 64, 128, 256, 512, 32]}},
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 1e-2,
            "weight_decay": 1e-4
            }},
    "batch_size": 4,
    "valid_interval": 2,
    "standard": "SSIM",
    "Metrics": {
        "SSIM": {"spatial_dims": 2,
                        "data_range": 6}},
    "early_stopping": 20,
    "criterion": "L1Loss",
    "use_amp": true
}